{
  "name": "stojg/sandbox.dev",
  "description": "This is stigs SilverStripe sandbox",
  "require": {
    "php": ">=5.6",
    "silverstripe/cms": "4.0.x-dev",
    "silverstripe/framework": "dev-patch-2 as 4.0.x-dev",
    "silverstripe/crontask": "*",
    "silverstripe/environmentcheck": "*",
    "silverstripe/dynamodb": "*",
    "silverstripe/asset-admin": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:andrewandante/silverstripe-framework.git"
    }
  ],
  "require-dev": {
    "phpunit/PHPUnit": "~4.8@stable"
  },
  "config": {
    "process-timeout": 600
  },
  "minimum-stability": "dev"
}
